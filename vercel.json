{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "api/cron_task.py": {
            "runtime": "python3.9",
            "schedule": "* * * * *"
        }
    }
}